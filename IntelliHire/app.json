{
  "expo": {
    "name": "IntelliHire",
    "slug": "IntelliHire",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,

