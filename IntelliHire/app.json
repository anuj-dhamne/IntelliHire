        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["INTERNET"],
      "package": "com.anuj_dhamne.IntelliHire"
    },

    "web": {
      "favicon": "./assets/Logo_mini.png"
    },

    "extra": {
      "eas": {
        "projectId": "ecf34db1-df43-4d88-929a-9afc3e10acc0"
      }
    },

    "owner": "anuj_dhamne"
  }
}
